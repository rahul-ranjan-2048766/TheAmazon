    <div class="p-5"></div>
    @{
        if(ViewBag.message != null)
        {
        <div class="alert alert-primary col-lg-6 m-auto" style="font-size: 12px; letter-spacing: 1px">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-cloud-drizzle-fill" viewBox="0 0 16 16">
                <path d="M4.158 12.025a.5.5 0 0 1 .316.633l-.5 1.5a.5.5 0 0 1-.948-.316l.5-1.5a.5.5 0 0 1 .632-.317zm6 0a.5.5 0 0 1 .316.633l-.5 1.5a.5.5 0 0 1-.948-.316l.5-1.5a.5.5 0 0 1 .632-.317zm-3.5 1.5a.5.5 0 0 1 .316.633l-.5 1.5a.5.5 0 0 1-.948-.316l.5-1.5a.5.5 0 0 1 .632-.317zm6 0a.5.5 0 0 1 .316.633l-.5 1.5a.5.5 0 1 1-.948-.316l.5-1.5a.5.5 0 0 1 .632-.317zm.747-8.498a5.001 5.001 0 0 0-9.499-1.004A3.5 3.5 0 1 0 3.5 11H13a3 3 0 0 0 .405-5.973z" />
            </svg>
            &nbsp;@ViewBag.message
        </div>
        }
     }
    <div class="p-2"></div>
    <div class="card_cover col-lg-6 m-auto p-4 row">
        <div class="col-lg-8 m-0 p-0">
            <p class="p_tag">Product Name: @Model.Item1.ProductName</p>
            <p class="p_tag">Category: @Model.Item1.ProductCategory</p>
            <p class="p_tag">Price: Rs @Model.Item1.Price</p>
            <p class="p_tag">Company: @Model.Item1.Company</p>            
        </div>
        <div class="col-lg-4 m-0 p-0">
            <img src="@Url.Content(Model.Item1.ImageUrl)" class="img-fluid image" />
        </div>
        <div class="p-2" style="border-bottom: 1px solid #80808040;"></div>
        <div class="p-2"></div>
        @using (Html.BeginForm("PlaceOrder", "Home", FormMethod.Post))
        {
            @Html.AntiForgeryToken()
            <p class="p_tag">Quantity Ordered: <span id="quantityOrdered">0</span></p>
            <p class="p_tag">Net Price: <span id="netPrice">Rs 0</span></p>
            <div class="p-2"></div>
            <div class="row m-0 p-0">
                <div class="col-lg-4 m-0 p-0"><label class="p_tag">Quantity</label></div>
                <div class="col-lg-8 m-0 p-0 position-relative">
                    <input type="number" name="QuantityOrdered" class="form-control input" placeholder="Please enter the quantity to be ordered" min="0" max="@Model.Item1.Quantity" onkeyup="((e) =>
{
    $('#quantityOrdered').html($(this).val());
    $('#netPrice').html($(this).val() + ' * Rs ' + @Model.Item1.Price + ' = ' + $(this).val() * @Model.Item1.Price);
})(event);" required />
                    <div class="position-absolute top-0 end-0 label_cover">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-layers-fill" viewBox="0 0 16 16">
                            <path d="M7.765 1.559a.5.5 0 0 1 .47 0l7.5 4a.5.5 0 0 1 0 .882l-7.5 4a.5.5 0 0 1-.47 0l-7.5-4a.5.5 0 0 1 0-.882l7.5-4z" />
                            <path d="m2.125 8.567-1.86.992a.5.5 0 0 0 0 .882l7.5 4a.5.5 0 0 0 .47 0l7.5-4a.5.5 0 0 0 0-.882l-1.86-.992-5.17 2.756a1.5 1.5 0 0 1-1.41 0l-5.17-2.756z" />
                        </svg>
                    </div>
                </div>
            </div>
            <div class="p-2"></div>
            <div class="row m-0 p-0">
                <div class="col-lg-4 m-0 p-0"><label class="p_tag">Address</label></div>
                <div class="col-lg-8 m-0 p-0 position-relative">
                    <input type="text" class="form-control input" placeholder="Please enter the delivery address" name="Address" required />
                    <div class="position-absolute top-0 end-0 label_cover">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-building" viewBox="0 0 16 16">
                            <path fill-rule="evenodd" d="M14.763.075A.5.5 0 0 1 15 .5v15a.5.5 0 0 1-.5.5h-3a.5.5 0 0 1-.5-.5V14h-1v1.5a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5V10a.5.5 0 0 1 .342-.474L6 7.64V4.5a.5.5 0 0 1 .276-.447l8-4a.5.5 0 0 1 .487.022zM6 8.694 1 10.36V15h5V8.694zM7 15h2v-1.5a.5.5 0 0 1 .5-.5h2a.5.5 0 0 1 .5.5V15h2V1.309l-7 3.5V15z" />
                            <path d="M2 11h1v1H2v-1zm2 0h1v1H4v-1zm-2 2h1v1H2v-1zm2 0h1v1H4v-1zm4-4h1v1H8V9zm2 0h1v1h-1V9zm-2 2h1v1H8v-1zm2 0h1v1h-1v-1zm2-2h1v1h-1V9zm0 2h1v1h-1v-1zM8 7h1v1H8V7zm2 0h1v1h-1V7zm2 0h1v1h-1V7zM8 5h1v1H8V5zm2 0h1v1h-1V5zm2 0h1v1h-1V5zm0-2h1v1h-1V3z" />
                        </svg>
                    </div>
                </div>
            </div>
            <div class="p-2"></div>
            <input type="hidden" value="@Model.Item1.Id" name="ProductId"/>
            <button class="place_order">Place</button>
        }
    </div>
    <div class="p-4"></div>

    <footer style="width: 100%; height: 40vh; background: #6495edd0;" class="d-flex flex-column justify-content-end">
