    <div class="p-2"></div>
    <div class="col-lg-8 m-auto p-0 card_cover row">
        @using (Html.BeginForm("UserProfile", "Home", FormMethod.Post, new { @class = "col-lg-7 m-0 p-4" }))
        {
            @Html.AntiForgeryToken()

