                                <div class="card-footer text-center" style="font-size: 10px; letter-spacing: 1px; color: #808080;">Date & Time Of Registration: @Model.user.DateTime</div>
                            </div>
                            <div class="p-2" style="border-bottom: 1px solid #80808040;"></div>
                            <div class="p-2"></div>
                            <div class="row m-0 p-0">
                                <div class="col-lg-8 m-0 p-0">
                                    <p class="p_tag">Product: @Model.product.ProductName</p>
                                    <p class="p_tag">Category: @Model.product.ProductCategory</p>
                                    <p class="p_tag">Company: @Model.product.Company</p>
                                    <p class="p_tag">Price: Rs @Model.product.Price</p>
                                    <p class="p_tag">Quantity Available: @Model.product.Quantity</p>
                                </div>
                                <div class="col-lg-4 m-0 p-0">
                                    <img src="@Url.Content(Model.product.ImageUrl)" class="img-fluid image" />
                                </div>
                                <div class="card-footer text-center" style="font-size: 10px; letter-spacing: 1px; color: #808080">Date & Time Of Entry: @Model.product.DateTime</div>
                            </div>
                            <div class="p-2" style="border-bottom: 1px solid #80808040;"></div>
                            <div class="p-2"></div>
                            <div class="m-0 p-0">
                                <p class="p_tag">Quantity Ordered: @Model.cart.QuantityOrdered</p>
                                <p class="p_tag">Net Price: Rs @Model.cart.NetPrice</p>
                                <p class="p_tag">Date & Time: @Model.cart.DateTime</p>
                                <p class="p_tag">Delivery Address: @Model.cart.DeliveryAddress</p>
                                <p class="p_tag">Status: @Model.cart.Status</p>
                            </div>
                            <div class="p-4"></div>
                            <div class="d-flex flex-row align-items-center justify-content-between m-0 p-0">
                                @if (Model.cart.Status.ToLower().Equals("delivered"))
                                {
                                    <button class="mark_undelivered" onclick="location.href='@Url.Action("MarkDelivered", "Home", new { CartId=Model.cart.Id })'">Mark Undelivered</button>
                                }
                                else
                                {
                                    <button class="mark_delivered" onclick="location.href='@Url.Action("MarkDelivered", "Home", new { CartId=Model.cart.Id })'">Mark Delivered</button>
                                }

                                <button class="remove_details" onclick="location.href='@Url.Action("RemoveDetails", "Home", new { CartId=Model.cart.Id })'">Remove Details</button>
                            </div>
                        </div>
                        <div class="p-5"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var trigger = $('.hamburger'),
                overlay = $('.overlay'),
                isClosed = false;

            trigger.click(function () {
                hamburger_cross();
            });

            function hamburger_cross() {

                if (isClosed == true) {
                    overlay.hide();
                    trigger.removeClass('is-open');
                    trigger.addClass('is-closed');
                    isClosed = false;
                } else {
                    overlay.show();
                    trigger.removeClass('is-closed');
                    trigger.addClass('is-open');
                    isClosed = true;
                }
            }

            $('[data-toggle="offcanvas"]').click(function () {
                $('#home').toggleClass('toggled');
            });
        });
    </script>
</body>
</html>