        }

        .p_tag {
            color: #808080;
            font-size: 12px;
            letter-spacing: 1px;
            line-height: 24px;
        }

        .image {
            width: 100%;
            height: 26vh;
        }

        .mark_delivered, .remove_details {
            cursor: pointer;
            padding: 10px 20px 10px 20px;
            font-size: 12px;
            letter-spacing: 1px;
            border: none;
            outline: none;
            color: #ffffff;
            box-shadow: rgba(0, 0, 0, 0.12) 0px 1px 3px, rgba(0, 0, 0, 0.24) 0px 1px 2px;
        }

         .mark_delivered{
             background: #191970a0;
         }

            .mark_delivered:hover {
                background: #191970d0;
            }

        .remove_details {
            background: #c71585a0;
        }

            .remove_details:hover {
                background: #c71585d0;
            }

        .mark_undelivered {
            cursor: pointer;
            padding: 10px 20px 10px 20px;
            font-size: 12px;
            letter-spacing: 1px;
            border: none;
            outline: none;
            color: #ffffff;
            box-shadow: rgba(0, 0, 0, 0.12) 0px 1px 3px, rgba(0, 0, 0, 0.24) 0px 1px 2px;
            background: #6495eda0;
        }

            .mark_undelivered:hover {
                background: #6495edd0;
            }
    </style>
</head>
